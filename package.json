{
	"name": "de.kernich.odpu",
	"version": "1.0.0",
	"description": "ODATA & APC Test Tool",
	"author": "Mario Kernich",
	"license": "Apache-2.0",
	"keywords": [
		"ui5",
		"openui5",
		"sapui5"
	],
	"main": "dist/Component.js",
	"types": "dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/mariokernich/odapu-ui5.git"
	},
	"scripts": {
		"clean": "rimraf dist coverage",
		"build": "ui5 build --clean-dest",
		"build:opt": "ui5 build self-contained --clean-dest --all",
		"build:mta": "npm run build:opt && mbt build",
		"build:ts-interfaces": "npx @ui5/ts-interface-generator",
		"generate:odata": "odata2ts",
		"start": "ui5 serve --port 8080 -o index.html",
		"start:dev": "ui5 serve --port 8080 --config=ui5-mock.yaml -o index.html",
		"deploy:dev": "npm run build && cross-env NODE_OPTIONS=\"--max-http-header-size=65536\" fiori deploy --yes --config ui5-deploy.yaml",
		"undeploy:dev": "fiori undeploy --config ui5-deploy.yaml",
		"ts-typecheck": "tsc --noEmit",
		"lint": "eslint webapp",
		"lint-fix": "eslint webapp --fix",
		"ui5lint": "ui5lint"
	},
	"devDependencies": {
		"@odata2ts/odata2ts": "^0.39.1",
		"@openui5/types": "^1.134.0",
		"@sap/ux-ui5-tooling": "^1.17.3",
		"@sapui5/types": "1.120.29",
		"@ui5/cli": "^4.0.8",
		"@ui5/linter": "^1.0.2",
		"@ui5/ts-interface-generator": "^0.10.0",
		"babel-plugin-istanbul": "^7.0.0",
		"chevrotain": "^11.0.3",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.3",
		"eslint": "^9.12.0",
		"globals": "^15.11.0",
		"mbt": "^1.2.34",
		"rimraf": "^6.0.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.8.1",
		"ui5-middleware-livereload": "^3.1.0",
		"ui5-middleware-serveframework": "^3.5.0",
		"ui5-middleware-simpleproxy": "^3.4.1",
		"ui5-tooling-modules": "^3.27.6",
		"ui5-tooling-transpile": "^3.5.0"
	},
	"ui5": {
		"dependencies": [
			"ui5-tooling-modules"
		]
	}
}
